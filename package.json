{
  "name": "echarts-comp",
  "description": "Apache Echarts components for React",
  "authors": "Eavid",
  "version": "0.0.5",
  "type": "module",
  "main": "dist/mod.cjs",
  "module": "dist/mod.js",
  "types": "dist/mod.d.ts",
  "peerDependencies": {
    "echarts": "^5.4.3",
    "react": "^18.2.0"
  },
  "exports": {
